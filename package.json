    "html-webpack-plugin": "^4.0.0-beta.11",
    "husky": "^3.1.0",
    "less": "^3.10.3",
    "less-loader": "^5.0.0",
    "lint-staged": "^9.5.0",
    "lru-cache": "^5.1.1",
    "mini-css-extract-plugin": "^0.8.0",
